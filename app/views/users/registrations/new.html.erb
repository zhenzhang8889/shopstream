<div class="five columns centered panel">
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <h3>Sign up</h3>

    <%= devise_error_messages! %>

    <%= f.email_field :email, placeholder: 'Email', class: 'twelve' %>

    <%= f.password_field :password, placeholder: 'Password', class: 'twelve' %>

    <%= f.password_field :password_confirmation, placeholder: 'Confirm', class: 'twelve' %>

    <%= f.submit "Sign up", class: 'button' %>
  <% end %>

  <%= render "users/shared/links" %>
</div>
